<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:papp="http://schemas.android.com/apk/res-auto"
    style="@style/gridLayout">

    <androidx.percentlayout.widget.PercentFrameLayout style="@style/perccentLayout">

        <RelativeLayout
            android:id="@+id/rel1"
            style="@style/gridNew"
            papp:layout_heightPercent="75.0%"
            papp:layout_marginLeftPercent="0.0%"
            papp:layout_marginTopPercent="0.0%"
            papp:layout_widthPercent="25.0%">

            <carbon.widget.FrameLayout
                android:id="@+id/frame1"
                style="@style/gridNew">

                <com.app.mylib.grid3d.PorterShapeImageView
                    android:id="@+id/image_add1"
                    style="@style/gridImage"
                    papp:siShape="@drawable/square_mask" />

            </carbon.widget.FrameLayout>

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rel2"
            style="@style/gridNew"
            papp:layout_heightPercent="25.0%"
            papp:layout_marginLeftPercent="0.0%"
            papp:layout_marginTopPercent="75.0%"
            papp:layout_widthPercent="75.0%">

            <carbon.widget.FrameLayout
                android:id="@+id/frame2"
                style="@style/gridNew">

                <com.app.mylib.grid3d.PorterShapeImageView
                    android:id="@+id/image_add2"
                    style="@style/gridImage"
                    papp:siShape="@drawable/square_mask" />

            </carbon.widget.FrameLayout>

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rel3"
            style="@style/gridNew"
            papp:layout_heightPercent="75.0%"
            papp:layout_marginLeftPercent="75.0%"
            papp:layout_marginTopPercent="25.0%"
            papp:layout_widthPercent="25.0%">

            <carbon.widget.FrameLayout
                android:id="@+id/frame3"
                style="@style/gridNew">

                <com.app.mylib.grid3d.PorterShapeImageView
                    android:id="@+id/image_add3"
                    style="@style/gridImage"
                    papp:siShape="@drawable/square_mask" />

            </carbon.widget.FrameLayout>

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rel4"
            style="@style/gridNew"
            papp:layout_heightPercent="25.0%"
            papp:layout_marginLeftPercent="25.0%"
            papp:layout_marginTopPercent="0.0%"
            papp:layout_widthPercent="75.0%">

            <carbon.widget.FrameLayout
                android:id="@+id/frame4"
                style="@style/gridNew">

                <com.app.mylib.grid3d.PorterShapeImageView
                    android:id="@+id/image_add4"
                    style="@style/gridImage"
                    papp:siShape="@drawable/square_mask" />

            </carbon.widget.FrameLayout>

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rel5"
            style="@style/gridNew"
            papp:layout_heightPercent="50.0%"
            papp:layout_marginLeftPercent="25.0%"
            papp:layout_marginTopPercent="25.0%"
            papp:layout_widthPercent="50.0%">

            <carbon.widget.FrameLayout
                android:id="@+id/frame5"
                style="@style/gridNew">

                <com.app.mylib.grid3d.PorterShapeImageView
                    android:id="@+id/image_add5"
                    style="@style/gridImage"
                    papp:siShape="@drawable/square_mask" />

            </carbon.widget.FrameLayout>

        </RelativeLayout>

    </androidx.percentlayout.widget.PercentFrameLayout>

</LinearLayout>