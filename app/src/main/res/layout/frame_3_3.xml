<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:papp="http://schemas.android.com/apk/res-auto"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical">

    <androidx.percentlayout.widget.PercentFrameLayout style="@style/perccentLayout">

        <com.app.mylib.grid3d.PorterShapeImageView
            android:id="@+id/touchImageView1"
            style="@style/touchImage"
            papp:layout_heightPercent="54.5%"
            papp:layout_marginLeftPercent="7.8%"
            papp:layout_marginTopPercent="2.7%"
            papp:layout_widthPercent="45.8%"
            papp:siShape="@drawable/frame3_3_1" />

        <com.app.mylib.grid3d.PorterShapeImageView
            android:id="@+id/touchImageView2"
            style="@style/touchImage"
            papp:layout_heightPercent="56.5%"
            papp:layout_marginLeftPercent="53.5%"
            papp:layout_marginTopPercent="37.0%"
            papp:layout_widthPercent="45.5%"
            papp:siShape="@drawable/frame3_3_2" />

        <com.app.mylib.grid3d.PorterShapeImageView
            android:id="@+id/touchImageView3"
            style="@style/touchImage"
            papp:layout_heightPercent="37.0%"
            papp:layout_marginLeftPercent="0.0%"
            papp:layout_marginTopPercent="57.5%"
            papp:layout_widthPercent="41.5%"
            papp:siShape="@drawable/frame3_3_3" />

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/mainImage"
            style="@style/mainImage"
            android:src="@drawable/frame3_3" />

    </androidx.percentlayout.widget.PercentFrameLayout>

</LinearLayout>