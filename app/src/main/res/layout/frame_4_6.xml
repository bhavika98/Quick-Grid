<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:papp="http://schemas.android.com/apk/res-auto"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical">

    <androidx.percentlayout.widget.PercentFrameLayout style="@style/perccentLayout">

        <com.app.mylib.grid3d.PorterShapeImageView
            android:id="@+id/touchImageView1"
            style="@style/touchImage"
            papp:layout_heightPercent="60.0%"
            papp:layout_marginLeftPercent="0.0%"
            papp:layout_marginTopPercent="0.0%"
            papp:layout_widthPercent="39.3%"
            papp:siShape="@drawable/frame4_6_1" />

        <com.app.mylib.grid3d.PorterShapeImageView
            android:id="@+id/touchImageView2"
            style="@style/touchImage"
            papp:layout_heightPercent="60.2%"
            papp:layout_marginLeftPercent="60.3%"
            papp:layout_marginTopPercent="0.0%"
            papp:layout_widthPercent="40.1%"
            papp:siShape="@drawable/frame4_6_2" />

        <com.app.mylib.grid3d.PorterShapeImageView
            android:id="@+id/touchImageView3"
            style="@style/touchImage"
            papp:layout_heightPercent="60.2%"
            papp:layout_marginLeftPercent="41.4%"
            papp:layout_marginTopPercent="40.1%"
            papp:layout_widthPercent="58.8%"
            papp:siShape="@drawable/frame4_6_3" />

        <com.app.mylib.grid3d.PorterShapeImageView
            android:id="@+id/touchImageView4"
            style="@style/touchImage"
            papp:layout_heightPercent="60.2%"
            papp:layout_marginLeftPercent="0.0%"
            papp:layout_marginTopPercent="40.0%"
            papp:layout_widthPercent="49.9%"
            papp:siShape="@drawable/frame4_6_4" />

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/mainImage"
            style="@style/mainImage"
            android:src="@drawable/frame4_6" />

    </androidx.percentlayout.widget.PercentFrameLayout>

</LinearLayout>