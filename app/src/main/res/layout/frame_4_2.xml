<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:papp="http://schemas.android.com/apk/res-auto"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical">

    <androidx.percentlayout.widget.PercentFrameLayout style="@style/perccentLayout">

        <com.app.mylib.grid3d.PorterShapeImageView
            android:id="@+id/touchImageView1"
            style="@style/touchImage"
            papp:layout_heightPercent="39.6%"
            papp:layout_marginLeftPercent="9.3%"
            papp:layout_marginTopPercent="9.6%"
            papp:layout_widthPercent="40.5%"
            papp:siShape="@drawable/frame4_2_1" />

        <com.app.mylib.grid3d.PorterShapeImageView
            android:id="@+id/touchImageView2"
            style="@style/touchImage"
            papp:layout_heightPercent="46.0%"
            papp:layout_marginLeftPercent="49.9%"
            papp:layout_marginTopPercent="4.4%"
            papp:layout_widthPercent="46.0%"
            papp:siShape="@drawable/frame4_2_2" />

        <com.app.mylib.grid3d.PorterShapeImageView
            android:id="@+id/touchImageView3"
            style="@style/touchImage"
            papp:layout_heightPercent="34.8%"
            papp:layout_marginLeftPercent="50.2%"
            papp:layout_marginTopPercent="51.1%"
            papp:layout_widthPercent="35.5%"
            papp:siShape="@drawable/frame4_2_3" />

        <com.app.mylib.grid3d.PorterShapeImageView
            android:id="@+id/touchImageView4"
            style="@style/touchImage"
            papp:layout_heightPercent="50.5%"
            papp:layout_marginLeftPercent="0.1%"
            papp:layout_marginTopPercent="49.5%"
            papp:layout_widthPercent="50.4%"
            papp:siShape="@drawable/frame4_2_4" />

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/mainImage"
            style="@style/mainImage"
            android:src="@drawable/frame4_2" />

    </androidx.percentlayout.widget.PercentFrameLayout>

</LinearLayout>