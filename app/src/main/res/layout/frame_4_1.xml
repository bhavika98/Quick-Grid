<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:papp="http://schemas.android.com/apk/res-auto"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical">

    <androidx.percentlayout.widget.PercentFrameLayout style="@style/perccentLayout">

        <com.app.mylib.grid3d.PorterShapeImageView
            android:id="@+id/touchImageView1"
            style="@style/touchImage"
            papp:layout_heightPercent="96.8%"
            papp:layout_marginLeftPercent="0.5%"
            papp:layout_marginTopPercent="1.5%"
            papp:layout_widthPercent="63.0%"
            papp:siShape="@drawable/frame1_2_1" />

        <com.app.mylib.grid3d.PorterShapeImageView
            android:id="@+id/touchImageView2"
            style="@style/touchImage"
            papp:layout_heightPercent="23.0%"
            papp:layout_marginLeftPercent="63.7%"
            papp:layout_marginTopPercent="27.8%"
            papp:layout_widthPercent="35.5%"
            papp:siShape="@drawable/frame1_2_1" />

        <com.app.mylib.grid3d.PorterShapeImageView
            android:id="@+id/touchImageView3"
            style="@style/touchImage"
            papp:layout_heightPercent="23.0%"
            papp:layout_marginLeftPercent="63.7%"
            papp:layout_marginTopPercent="51.5%"
            papp:layout_widthPercent="35.5%"
            papp:siShape="@drawable/frame1_2_1" />

        <com.app.mylib.grid3d.PorterShapeImageView
            android:id="@+id/touchImageView4"
            style="@style/touchImage"
            papp:layout_heightPercent="23.0%"
            papp:layout_marginLeftPercent="63.7%"
            papp:layout_marginTopPercent="75.3%"
            papp:layout_widthPercent="35.5%"
            papp:siShape="@drawable/frame1_2_1" />

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/mainImage"
            style="@style/mainImage"
            android:src="@drawable/frame4_1" />

    </androidx.percentlayout.widget.PercentFrameLayout>

</LinearLayout>