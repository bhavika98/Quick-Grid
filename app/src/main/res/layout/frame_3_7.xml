<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:papp="http://schemas.android.com/apk/res-auto"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical">

    <androidx.percentlayout.widget.PercentFrameLayout style="@style/perccentLayout">

        <com.app.mylib.grid3d.PorterShapeImageView
            android:id="@+id/touchImageView1"
            style="@style/touchImage"
            papp:layout_heightPercent="36.0%"
            papp:layout_marginLeftPercent="47.2%"
            papp:layout_marginTopPercent="0.0%"
            papp:layout_widthPercent="53.1%"
            papp:siShape="@drawable/frame3_7_1" />

        <com.app.mylib.grid3d.PorterShapeImageView
            android:id="@+id/touchImageView2"
            style="@style/touchImage"
            papp:layout_heightPercent="36.2%"
            papp:layout_marginLeftPercent="0.0%"
            papp:layout_marginTopPercent="25.6%"
            papp:layout_widthPercent="53.5%"
            papp:siShape="@drawable/frame3_7_3" />

        <com.app.mylib.grid3d.PorterShapeImageView
            android:id="@+id/touchImageView3"
            style="@style/touchImage"
            papp:layout_heightPercent="33.2%"
            papp:layout_marginLeftPercent="42.2%"
            papp:layout_marginTopPercent="61.0%"
            papp:layout_widthPercent="58.5%"
            papp:siShape="@drawable/frame3_7_2" />

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/mainImage"
            style="@style/mainImage"
            android:src="@drawable/frame3_7" />

    </androidx.percentlayout.widget.PercentFrameLayout>

</LinearLayout>