<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:papp="http://schemas.android.com/apk/res-auto"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical">

    <androidx.percentlayout.widget.PercentFrameLayout style="@style/perccentLayout">

        <com.app.mylib.grid3d.PorterShapeImageView
            android:id="@+id/touchImageView1"
            style="@style/touchImage"
            papp:layout_heightPercent="49.0%"
            papp:layout_marginLeftPercent="3.0%"
            papp:layout_marginTopPercent="0.9%"
            papp:layout_widthPercent="48.5%"
            papp:siShape="@drawable/frame3_4_1" />

        <com.app.mylib.grid3d.PorterShapeImageView
            android:id="@+id/touchImageView2"
            style="@style/touchImage"
            papp:layout_heightPercent="47.6%"
            papp:layout_marginLeftPercent="3.0%"
            papp:layout_marginTopPercent="49.7%"
            papp:layout_widthPercent="47.8%"
            papp:siShape="@drawable/frame3_4_1" />

        <com.app.mylib.grid3d.PorterShapeImageView
            android:id="@+id/touchImageView3"
            style="@style/touchImage"
            papp:layout_heightPercent="79.0%"
            papp:layout_marginLeftPercent="53.3%"
            papp:layout_marginTopPercent="2.0%"
            papp:layout_widthPercent="44.0%"
            papp:siShape="@drawable/frame1_2_1" />

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/mainImage"
            style="@style/mainImage"
            android:src="@drawable/frame3_4" />

    </androidx.percentlayout.widget.PercentFrameLayout>

</LinearLayout>