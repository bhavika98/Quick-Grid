<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:papp="http://schemas.android.com/apk/res-auto"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical">

    <androidx.percentlayout.widget.PercentFrameLayout style="@style/perccentLayout">

        <com.app.mylib.grid3d.PorterShapeImageView
            android:id="@+id/touchImageView1"
            style="@style/touchImage"
            papp:layout_heightPercent="51.8%"
            papp:layout_marginLeftPercent="14.0%"
            papp:layout_marginTopPercent="1.1%"
            papp:layout_widthPercent="86.0%"
            papp:siShape="@drawable/frame2_6_1" />

        <com.app.mylib.grid3d.PorterShapeImageView
            android:id="@+id/touchImageView2"
            style="@style/touchImage"
            papp:layout_heightPercent="53.0%"
            papp:layout_marginLeftPercent="0.5%"
            papp:layout_marginTopPercent="45.7%"
            papp:layout_widthPercent="88.0%"
            papp:siShape="@drawable/frame2_6_2" />

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/mainImage"
            style="@style/mainImage"
            android:src="@drawable/frame2_6" />

    </androidx.percentlayout.widget.PercentFrameLayout>

</LinearLayout>