<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:papp="http://schemas.android.com/apk/res-auto"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical">

    <androidx.percentlayout.widget.PercentFrameLayout style="@style/perccentLayout">

        <com.app.mylib.grid3d.PorterShapeImageView
            android:id="@+id/touchImageView1"
            style="@style/touchImage"
            papp:layout_heightPercent="48.4%"
            papp:layout_marginLeftPercent="5.3%"
            papp:layout_marginTopPercent="4.0%"
            papp:layout_widthPercent="50.0%"
            papp:siShape="@drawable/frame3_2_1" />

        <com.app.mylib.grid3d.PorterShapeImageView
            android:id="@+id/touchImageView2"
            style="@style/touchImage"
            papp:layout_heightPercent="49.3%"
            papp:layout_marginLeftPercent="45.5%"
            papp:layout_marginTopPercent="45.0%"
            papp:layout_widthPercent="50.3%"
            papp:siShape="@drawable/frame3_2_2" />

        <com.app.mylib.grid3d.PorterShapeImageView
            android:id="@+id/touchImageView3"
            style="@style/touchImage"
            papp:layout_heightPercent="33.4%"
            papp:layout_marginLeftPercent="59.6%"
            papp:layout_marginTopPercent="5.5%"
            papp:layout_widthPercent="34.6%"
            papp:siShape="@drawable/frame3_2_2" />

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/mainImage"
            style="@style/mainImage"
            android:src="@drawable/frame3_2" />

    </androidx.percentlayout.widget.PercentFrameLayout>

</LinearLayout>