<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:papp="http://schemas.android.com/apk/res-auto"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical">

    <androidx.percentlayout.widget.PercentFrameLayout style="@style/perccentLayout">

        <com.app.mylib.grid3d.PorterShapeImageView
            android:id="@+id/touchImageView1"
            style="@style/touchImage"
            papp:layout_heightPercent="29.0%"
            papp:layout_marginLeftPercent="0.0%"
            papp:layout_marginTopPercent="12.0%"
            papp:layout_widthPercent="30.0%"
            papp:siShape="@drawable/frame5_3_1" />

        <com.app.mylib.grid3d.PorterShapeImageView
            android:id="@+id/touchImageView2"
            style="@style/touchImage"
            papp:layout_heightPercent="61.5%"
            papp:layout_marginLeftPercent="0.0%"
            papp:layout_marginTopPercent="27.0%"
            papp:layout_widthPercent="61.5%"
            papp:siShape="@drawable/frame5_3_1" />

        <com.app.mylib.grid3d.PorterShapeImageView
            android:id="@+id/touchImageView3"
            style="@style/touchImage"
            papp:layout_heightPercent="40.7%"
            papp:layout_marginLeftPercent="41.2%"
            papp:layout_marginTopPercent="58.3%"
            papp:layout_widthPercent="41.5%"
            papp:siShape="@drawable/frame5_3_1" />

        <com.app.mylib.grid3d.PorterShapeImageView
            android:id="@+id/touchImageView4"
            style="@style/touchImage"
            papp:layout_heightPercent="38.0%"
            papp:layout_marginLeftPercent="20.0%"
            papp:layout_marginTopPercent="0.0%"
            papp:layout_widthPercent="46.5%"
            papp:siShape="@drawable/frame5_3_2" />

        <com.app.mylib.grid3d.PorterShapeImageView
            android:id="@+id/touchImageView5"
            style="@style/touchImage"
            papp:layout_heightPercent="78.5%"
            papp:layout_marginLeftPercent="43.0%"
            papp:layout_marginTopPercent="0.0%"
            papp:layout_widthPercent="57.7%"
            papp:siShape="@drawable/frame5_3_3" />

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/mainImage"
            style="@style/mainImage"
            android:src="@drawable/frame5_3" />

    </androidx.percentlayout.widget.PercentFrameLayout>

</LinearLayout>