<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:papp="http://schemas.android.com/apk/res-auto"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical">

    <androidx.percentlayout.widget.PercentFrameLayout style="@style/perccentLayout">

        <com.app.mylib.grid3d.PorterShapeImageView
            android:id="@+id/touchImageView1"
            style="@style/touchImage"
            papp:layout_heightPercent="63.2%"
            papp:layout_marginLeftPercent="2.0%"
            papp:layout_marginTopPercent="18.3%"
            papp:layout_widthPercent="47.3%"
            papp:siShape="@drawable/frame3_6_1" />

        <com.app.mylib.grid3d.PorterShapeImageView
            android:id="@+id/touchImageView2"
            style="@style/touchImage"
            papp:layout_heightPercent="46.9%"
            papp:layout_marginLeftPercent="50.5%"
            papp:layout_marginTopPercent="2.1%"
            papp:layout_widthPercent="47.5%"
            papp:siShape="@drawable/frame3_6_2" />

        <com.app.mylib.grid3d.PorterShapeImageView
            android:id="@+id/touchImageView3"
            style="@style/touchImage"
            papp:layout_heightPercent="46.9%"
            papp:layout_marginLeftPercent="50.5%"
            papp:layout_marginTopPercent="51.0%"
            papp:layout_widthPercent="47.5%"
            papp:siShape="@drawable/frame3_6_3" />

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/mainImage"
            style="@style/mainImage"
            android:src="@drawable/frame3_6" />

    </androidx.percentlayout.widget.PercentFrameLayout>

</LinearLayout>