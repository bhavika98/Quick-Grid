<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:papp="http://schemas.android.com/apk/res-auto"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical">

    <androidx.percentlayout.widget.PercentFrameLayout style="@style/perccentLayout">

        <com.app.mylib.grid3d.PorterShapeImageView
            android:id="@+id/touchImageView1"
            style="@style/touchImage"
            papp:layout_heightPercent="35.0%"
            papp:layout_marginLeftPercent="8.0%"
            papp:layout_marginTopPercent="4.3%"
            papp:layout_widthPercent="86.0%"
            papp:siShape="@drawable/frame2_8_1" />

        <com.app.mylib.grid3d.PorterShapeImageView
            android:id="@+id/touchImageView2"
            style="@style/touchImage"
            papp:layout_heightPercent="34.7%"
            papp:layout_marginLeftPercent="7.0%"
            papp:layout_marginTopPercent="52.0%"
            papp:layout_widthPercent="86.0%"
            papp:siShape="@drawable/frame2_8_2" />

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/mainImage"
            style="@style/mainImage"
            android:src="@drawable/frame2_8" />

    </androidx.percentlayout.widget.PercentFrameLayout>

</LinearLayout>