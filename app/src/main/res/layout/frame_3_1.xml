<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:papp="http://schemas.android.com/apk/res-auto"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical">

    <androidx.percentlayout.widget.PercentFrameLayout style="@style/perccentLayout">

        <com.app.mylib.grid3d.PorterShapeImageView
            android:id="@+id/touchImageView1"
            style="@style/touchImage"
            papp:layout_heightPercent="100.0%"
            papp:layout_marginLeftPercent="0.0%"
            papp:layout_marginTopPercent="13.0%"
            papp:layout_widthPercent="33.6%"
            papp:siShape="@drawable/frame3_1_1" />

        <com.app.mylib.grid3d.PorterShapeImageView
            android:id="@+id/touchImageView2"
            style="@style/touchImage"
            papp:layout_heightPercent="87.5%"
            papp:layout_marginLeftPercent="33.5%"
            papp:layout_marginTopPercent="0.0%"
            papp:layout_widthPercent="33.3%"
            papp:siShape="@drawable/frame3_1_2" />

        <com.app.mylib.grid3d.PorterShapeImageView
            android:id="@+id/touchImageView3"
            style="@style/touchImage"
            papp:layout_heightPercent="87.5%"
            papp:layout_marginLeftPercent="66.5%"
            papp:layout_marginTopPercent="13.0%"
            papp:layout_widthPercent="33.5%"
            papp:siShape="@drawable/frame3_1_1" />

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/mainImage"
            style="@style/mainImage"
            android:src="@drawable/frame3_1" />

    </androidx.percentlayout.widget.PercentFrameLayout>

</LinearLayout>